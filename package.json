{
  "name": "node-pdf-generator",
  "version": "1.0.1",
  "description": "Node.js service for generating PDF via URL.",
  "main": "server.js",
  "scripts": {
    "precommit": "eslint --config ./.eslintrc.json --ignore-path ./.gitignore .",
    "start": "nodemon server.js",
    "dev": "run-s precommit start",
    "prod": "node server.js"
  },
  "nodemonConfig": {
    "delay": "1500"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kobzzz/node-pdf-generator.git"
  },
  "keywords": [
    "Puppeteer",
    "Node.js",
    "Docker"
  ],
  "author": "Evgeny Kobzev",
  "license": "ISC",
  "homepage": "https://github.com/kobzzz/node-pdf-generator.git#readme",
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-node": "^4.0.0",
    "eslint-plugin-import": "^2.17.3",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "finalhandler": "^1.1.2",
    "minimist": "^1.2.3",
    "morgan": "^1.9.1",
    "puppeteer": "^1.17.0"
  }
}
